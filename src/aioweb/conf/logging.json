{
    "disable_existing_loggers": false, 
    "formatters": {
        "standard": {
            "format": "%(asctime)s [%(levelname)s] %(name)s: %(message)s"
        }
    }, 
    "incremental": true, 
    "loggers": {
        "":{
            "propagate": false, 
            "handlers": [
                "console"
            ], 
            "level": "DEBUG"
        },
        "asyncio": {
            "propagate": false, 
            "handlers": [
                "console"
            ], 
            "level": "WARN"
        }, 
        "config": {
            "propagate": false, 
            "handlers": [
                "console"
            ], 
            "level": "WARN"
        }, 
        "CouchDBAdapter": {
            "propagate": false, 
            "handlers": [
                "console"
            ], 
            "level": "WARN"
        }
    }, 
    "version": 1
}
